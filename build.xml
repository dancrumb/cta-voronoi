<project name="CTA Voronoi" basedir=".">
    <property name="src" location="src"/>
    <property name="modules" location="modules"/>

    <target name="prebuild">
        <mkdir dir="${src}/lodash"/>
        <copy file="${modules}/lodash/lodash.min.js" todir="${src}/lodash"/>

        <mkdir dir="${src}/d3"/>
        <copy file="${modules}/d3/d3.min.js" todir="${src}/d3"/>

        <mkdir dir="${src}/less"/>
        <copy file="${modules}/less/dist/less-1.3.3.min.js" todir="${src}/less"/>
    </target>
</project>